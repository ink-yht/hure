[{"title":"index.lhtml","id":"173383180813146","mode":"html","href":"index.lhtml","content":["<页面>","    <区块 类名=\"首页主区块\">","        <!--顶部-->","        <区块 类名=\"首页顶部区域\">","            <区块 行内样式=\"宽度: 100%; 高度: 90rpx\"></区块>","            <区块 类名=\"顶部标题\">","                <文本框>{{数据层.顶部标题}}</文本框>","            </区块>","            <区块 类名=\"顶部搜索\">","                <按钮 类名=\"顶部搜索按钮\" @click=\"方法层.打开搜索页面\">","                    <图片框 图像地址=\"@文件_首页.png\" 类名=\"搜索图标\" 缩放模式=\"缩放图像\"></图片框>","                    <区块 类名=\"搜索标题\">搜索</区块>","                </按钮>","            </区块>","        </区块>","        <!--分类-->","        <区块 类名=\"首页分类\">","            <区块 类名=\"首页分类子类\">","                <区块 类名=\"首页分类子类1\">","                    <区块 类名=\"首页分类子类1标题\">","                        <文本框>{{数据层.首页分类子类1标题}}</文本框>","                    </区块>","                </区块>","                <区块 类名=\"首页分类子类2\">","                    <区块 类名=\"首页分类子类1标题\">","                        <文本框>{{数据层.首页分类子类2标题}}</文本框>","                    </区块>","                </区块>","            </区块>","        </区块>","        <!--招聘信息-->","        <区块 类名=\"首页招聘信息\">","            <区块 循环渲染=\"(数据,索引) 遍历 数据层.招聘数据\" :维护状态=\"数据.id\">","                <区块 行内样式=\"下边距: 20rpx;\">","                    <!--左边-->","                    <区块 类名=\"首页招聘信息子类 左侧招聘信息子类\">","                        <!--背景区域-->","                        <区块 类名=\"招聘信息背景 招聘信息背景图\"></区块>","                        <!--标题区域-->","                        <区块 类名=\"招聘信息标题\">","                            <区块 类名=\"招聘信息标题效果\">{{数据.L1.标题}}</区块>","                            <区块 类名=\"招聘信息效果\">","                                <文本框 类名=\"薪资\">{{数据.L1.薪资}}/日</文本框>","                                <文本框 类名=\"地址\">{{数据.L1.薪资}}</文本框>","                            </区块>","                        </区块>","                    </区块>","                    <!--右边-->","                    <区块 类名=\"首页招聘信息子类 右侧招聘信息子类\">","                        <!--背景区域-->","                        <区块 类名=\"招聘信息背景 招聘信息背景图\"></区块>","                        <!--标题区域-->","                        <区块 类名=\"招聘信息标题\">","                            <区块 类名=\"招聘信息标题效果\">{{数据.L2.标题}}</区块>","                            <区块 类名=\"招聘信息效果\">","                                <文本框 类名=\"薪资\">{{数据.L2.薪资}}/日</文本框>","                                <文本框 类名=\"地址\">{{数据.L2.薪资}}</文本框>","                            </区块>","                        </区块>","                    </区块>","                </区块>","            </区块>","        </区块>","    </区块>","</页面>","<脚本>","    局部变量 数据层 = {","        //所有的页面数据,请在本处定","        顶部标题: \"蓝领岗位招聘\",","        首页分类子类1标题: \"外卖到家\",","        首页分类子类2标题: \"快递到家\",","        招聘数据: [","            {","                \"L1\": {","                    \"id\": 0,","                    \"标题\": \"快码1\",","                    \"薪资\": 600,","                    \"地址\": \"城北区\"","                },","                \"L2\": {","                    \"id\": 1,","                    \"标题\": \"快码2\",","                    \"薪资\": 600,","                    \"地址\": \"城北区\"","                }","            },","            {","                \"L1\": {","                    \"id\": 2,","                    \"标题\": \"快码3\",","                    \"薪资\": 600,","                    \"地址\": \"城北区\"","                },","                \"L2\": {","                    \"id\": 3,","                    \"标题\": \"快码4\",","                    \"薪资\": 600,","                    \"地址\": \"城北区\"","                }","            },","            {","                \"L1\": {","                    \"id\": 4,","                    \"标题\": \"快码1\",","                    \"薪资\": 600,","                    \"地址\": \"城北区\"","                },","                \"L2\": {","                    \"id\": 5,","                    \"标题\": \"快码2\",","                    \"薪资\": 600,","                    \"地址\": \"城北区\"","                }","            },","            {","                \"L1\": {","                    \"id\": 6,","                    \"标题\": \"快码3\",","                    \"薪资\": 600,","                    \"地址\": \"城北区\"","                },","                \"L2\": {","                    \"id\": 7,","                    \"标题\": \"快码4\",","                    \"薪资\": 600,","                    \"地址\": \"城北区\"","                }","            },","            {","                \"L1\": {","                    \"id\": 8,","                    \"标题\": \"快码1\",","                    \"薪资\": 600,","                    \"地址\": \"城北区\"","                },","                \"L2\": {","                    \"id\": 9,","                    \"标题\": \"快码2\",","                    \"薪资\": 600,","                    \"地址\": \"城北区\"","                }","            },","            {","                \"L1\": {","                    \"id\": 10,","                    \"标题\": \"快码3\",","                    \"薪资\": 600,","                    \"地址\": \"城北区\"","                },","                \"L2\": {","                    \"id\": 11,","                    \"标题\": \"快码4\",","                    \"薪资\": 600,","                    \"地址\": \"城北区\"","                }","            }","        ]","    };","</脚本>","<脚本>","    局部变量 方法层 = {","        //所有页面方法,请在本处定义","","        打开搜索页面: 定义函数 () {","            跨平台类.显示提示框(\"你好,世界\");","        }","    };","</脚本>","<样式></样式>"],"line":{"row":34,"column":36},"first":false,"active":false,"load":true},{"title":"首页样式.lcss","id":"1733835224647","mode":"css","href":"首页样式.lcss","content":"/*整体*/\n.首页主区块 {\n    背景-颜色: #efefef;\n    宽度: @单位_填满宽度;\n    高度: @单位_填满高度;\n}\n/*首页顶部*/\n.首页顶部区域 {\n    宽度: 100%;\n    高度: 250rpx;\n    背景-颜色: 颜色值-宝蓝色;\n}\n\n.顶部标题 {\n    宽度: 100%;\n    高度: 60rpx;\n    显示模式: 弹性布局;\n    对齐-项目: 居中;\n    内容对齐: 居中;\n    字体-大小: 35px;\n}\n\n.顶部搜索 {\n    上边距: 20rpx;\n    下边距: 20rpx;\n    左边距: 20rpx;\n    右边距: 20rpx;\n    高度: 60rpx;\n}\n\n.顶部搜索按钮 {\n    宽度: 100%;\n    高度: 100%;\n    显示模式: 弹性布局;\n}\n.搜索图标 {\n    宽度: 40rpx;\n    高度: 40rpx;\n    外边距: 10rpx 0;\n}\n.搜索标题 {\n    显示模式: 弹性布局;\n    对齐-项目: 居中;\n    内容对齐: 居中;\n    左边距: 15rpx;\n}\n\n/*首页分类*/\n.首页分类 {\n    宽度: 100%;\n    高度: 200rpx;\n    定位: 相对定位;\n}\n.首页分类子类 {\n    定位: 绝对定位;\n    顶部: 20rpx;\n    左侧: 20rpx;\n    高度: 160rpx;\n    宽度: 710rpx;\n}\n.首页分类子类1 {\n    定位: 绝对定位;\n    顶部: 20rpx;\n    左侧: 20rpx;\n    高度: 120rpx;\n    宽度: 325rpx;\n    背景-颜色: 颜色值-黄色;\n    边框-圆角: 10rpx;\n}\n.首页分类子类1标题 {\n    定位: 绝对定位;\n    显示模式: 弹性布局;\n    顶部: 30rpx;\n    左侧: 37.5rpx;\n    对齐-项目: 居中;\n    内容对齐: 居中;\n    高度: 60rpx;\n    宽度: 250rpx;\n    边框-圆角: 10rpx;\n}\n\n.首页分类子类2 {\n    定位: 绝对定位;\n    顶部: 20rpx;\n    右侧: 20rpx;\n    高度: 120rpx;\n    宽度: 325rpx;\n    背景-颜色: 颜色值-黄色;\n    边框-圆角: 10rpx;\n}\n\n/*首页招聘信息*/\n.首页招聘信息 {\n    上边距: 20rpx;\n    宽度: 100%;\n}\n.招聘信息背景图 {\n    背景-图像: 地址(\"@文件_背景.jpg\");\n    背景-大小: 宽高覆盖缩放;\n}\n.首页招聘信息子类 {\n    显示模式: 内联块元素;\n    垂直-对齐: 顶部;\n    高度: 300rpx;\n    边框-圆角: 10rpx;\n}\n.左侧招聘信息子类 {\n    宽度: 计算(100% - 50% - 30rpx);\n    左边距: 20rpx;\n    右边距: 10rpx;\n    背景-颜色: 颜色值-白色;\n}\n.右侧招聘信息子类 {\n    宽度: 计算(100% - 50% - 30rpx);\n    左边距: 10rpx;\n    右边距: 20rpx;\n    背景-颜色: 颜色值-白色;\n}\n.招聘信息背景 {\n    高度: 70%;\n    宽度: 100%;\n    背景-颜色: 颜色值-粉色;\n    边框-圆角: 10rpx 10rpx 0 0;\n}\n.招聘信息标题 {\n    高度: 30%;\n    宽度: 100%;\n    边框-圆角: 0rpx 0rpx 10rpx 10rpx;\n}\n.招聘信息标题效果 {\n    左边距: 10rpx;\n    右边距: 10rpx;\n    上边距: 5rpx;\n}\n.招聘信息效果 {\n    定位: 相对定位;\n}\n.薪资 {\n    定位: 绝对定位;\n    左侧: 10rpx;\n    顶部: 25rpx;\n}\n.地址 {\n    定位: 绝对定位;\n    右侧: 10rpx;\n    顶部: 25rpx;\n}","line":{"row":52,"column":13},"IfOverall":0,"first":false,"active":false,"load":true},{"title":"登录页面.lhtml","id":"1734182159907","mode":"html","href":"登录页面.lhtml","content":["<页面>","\t","</页面>","<脚本>","\t局部变量 数据层 = {","\t\t//所有的页面数据,请在本处定义","\t\t","\t};","</脚本>","<脚本>","\t局部变量 方法层 = {","\t\t//所有页面方法,请在本处定义","\t\t","\t};","</脚本>","<样式>","\t","</样式>"],"line":{"row":0,"column":0},"IfOverall":0,"first":false,"active":true,"load":true}]